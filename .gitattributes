models/*.pkl filter=lfs diff=lfs merge=lfs -text
data/processed/*.pkl filter=lfs diff=lfs merge=lfs -text
*.pkl filter=lfs diff=lfs merge=lfs -text

# Mark Python files as the main language
*.py linguist-language=Python

# Ignore specific files or directories from language detection
Makefile linguist-vendored
*.md linguist-documentation
*.txt linguist-documentation
*.sh linguist-vendored
